{
  "name": "lazyfrosch-roundcube",
  "version": "0.0.0",
  "source": "https://github.com/lazyfrosch/puppet-roundcube.git",
  "author": "Markus Frosch <markus@lazyfrosch.de>",
  "license": "Apache License, Version 2.0",
  "summary": "Roundcube webmail",
  "description": "This module provides the necessary classes to install a configurable roundcube webmail service.",
  "project_page": "https://github.com/lazyfrosch/puppet-roundcube",
  "dependencies": [
    {
      "name": "puppetlabs/apache",
      "version_requirement": ">= 1.0.0"
    },
    {
      "name": "puppetlabs/inifile",
      "version_requirement": ">= 1.0.0"
    },
    {
      "name": "puppetlabs/postgresql",
      "version_requirement": ">= 3.3.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 1.4.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 2.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "15.04"
      ]
    }
  ]
}
